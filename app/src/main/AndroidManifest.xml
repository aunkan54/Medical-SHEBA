<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loginapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/green_leafs"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Navigation_Drawer"></activity>
        <activity android:name=".Ambulance_driver_view" />
        <activity android:name=".View_user_info" />
        <activity android:name=".Get_user_Info" />
        <activity android:name=".Profile" />
        <activity
            android:name=".Sign_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Sign_in"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Splash_Screen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>